{
  "name": "the-vision-client",
  "description": "",
  "version": "1.0.0",
  "license": "Apache-2.0",
  "author": "Louis Beaumont",
  "engines": {
    "node": ">=8"
  },
  "repository": "https://github.com/louis030195/vision-client",
  "main": "app.js",
  "cloud-repo-tools": {
    "requiresKeyFile": true,
    "requiresProjectId": true,
    "test": {
      "app": {
        "args": [
          "app.js"
        ],
        "msg": "vision"
      }
    }
  },
  "scripts": {
    "start": "node app.js",
    "start-full": "npm run build && node app.js",
    "start-back": "node app.js",
    "start-front": "webpack-dev-server  --config front/webpack.config.js --env development",
    "test": "repo-tools test app && ava -t 30s --tap test/*.test.js",
    "e2e": "repo-tools test deploy && E2E_TESTS=True ava -t 30s --tap test/*.test.js",
    "build": "webpack --config front/webpack.config.js --env production",
    "lint": "standard",
    "fix": "standard --fix",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@google-cloud/connect-datastore": "^3.0.0",
    "@google-cloud/datastore": "^4.0.0",
    "@google-cloud/storage": "^1.7.0",
    "@webcomponents/webcomponentsjs": "^2.2.10",
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "lit-element": "^2.2.0",
    "lit-html": "^1.1.1",
    "lodash": "^4.17.11",
    "multer": "^1.4.1",
    "nconf": "^0.10.0",
    "npm": "^6.8.0",
    "page": "^1.11.4",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^2.0.0",
    "prompt": "^1.0.0",
    "throttled-queue": "^1.0.6",
    "uglify-js": "^3.4.9"
  },
  "devDependencies": {
    "@google-cloud/nodejs-repo-tools": "^3.2.0",
    "ava": "^0.25.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^3.1.0",
    "file-loader": "^4.1.0",
    "html-webpack-plugin": "^3.2.0",
    "proxyquire": "^2.1.0",
    "raw-loader": "^3.1.0",
    "sinon": "^7.0.0",
    "standard": "^13.0.2",
    "style-loader": "^0.23.1",
    "supertest": "^4.0.0",
    "to-string-loader": "^1.1.5",
    "webpack": "^4.36.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.1"
  }
}
